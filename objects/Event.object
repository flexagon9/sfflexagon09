    <validationRules>
        <fullName>FollowUp_Task_Reminder_DateTime</fullName>
        <active>true</active>
        <description>Validates the user enters a reminder date/time if creating a follow-up task</description>
        <errorConditionFormula>AND(FollowUp_Create_Task__c, ISBLANK(FollowUp_Task_Reminder__c))</errorConditionFormula>
        <errorDisplayField>FollowUp_Task_Reminder__c</errorDisplayField>
        <errorMessage>Please enter a reminder date/time for the follow up task</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FollowUp_Task_Validate_Create_Field</fullName>
        <active>true</active>
        <description>Prevents the user from unchecking the Create Follow-Up checkbox if the task was already created</description>
        <errorConditionFormula>AND(
NOT( FollowUp_Create_Task__c ),
PRIORVALUE(FollowUp_Create_Task__c)
)</errorConditionFormula>
        <errorDisplayField>FollowUp_Create_Task__c</errorDisplayField>
        <errorMessage>Once the follow up task was created, this field cannot be unchecked</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>External_Email</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>External Email</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var body =&apos;{!Event.Minutes_AD_Customer__c}&apos;+&apos;%0A%0A&apos;+ 
&apos;{!Event.Goal_of_Next_Visit__c}&apos; ;

window.top.location.href=&apos;/_ui/core/email/author/EmailAuthor?retURL={!Event.Id}&amp;p3_lkid={!Event.What}&amp;p7=&apos;+body;</url>
    </webLinks>
    <webLinks>
        <fullName>Internal_Email</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send an Email</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 

var result = sforce.connection.query(&quot;SELECT LongTextNotes__c FROM ObjectExtender__c WHERE ExternalId__c = &apos;{!CASESAFEID(Event.Id)}&apos;&quot;); 

var body = &quot;&quot;; 
if (result.size != &quot;0&quot;) { 
    var record = result.getArray(&quot;records&quot;)[0]; 
    body = record.LongTextNotes__c; 
} 

window.top.location.href=&apos;/_ui/core/email/author/EmailAuthor?retURL={!Event.Id}&amp;p3_lkid={!Event.What}&amp;p7=&apos;+encodeURIComponent(body);</url>
    </webLinks>
